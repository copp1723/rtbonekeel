{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "reporter": ["text", "html", "json-summary"],
  "include": ["src/**/*.ts"],
  "exclude": ["**/*.d.ts", "tests/**", "**/*.spec.ts"],
  "check-coverage": true,
  "statements": 80,
  "branches": 80,
  "functions": 80,
  "lines": 80
}
